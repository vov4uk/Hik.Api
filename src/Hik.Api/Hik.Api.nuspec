<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <title>$title$</title>
    <authors>vov4uk</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/vov4uk/Hik.Api</projectUrl>
    <repository type="git" url="https://github.com/vov4uk/Hik.Api" branch="main" />
    <description>$description$</description>
    <releaseNotes>Added ConfigService, remove Session class, hide UserId parameter</releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>Hikvision, SDK, ipcamera, photo, video</tags>
    <icon>images\logo.png</icon>
    <contentFiles>
      <!-- this sets "CopyIfNewer" on all files in the project that references this package -->
      <files include="**/*" buildAction="None" copyToOutput="true"/>
    </contentFiles>
    <dependencies>
      <group targetFramework=".NETStandard2.1"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\logo.png" target="images\" />
    <file src="bin\$configuration$\netstandard2.1\SDK\*.*" target="contentFiles\any\any\SDK"/>
    <file src="bin\$configuration$\netstandard2.1\SDK\HCNetSDKCom\*.*" target="contentFiles\any\any\SDK\HCNetSDKCom\"/>
  </files>
</package>
